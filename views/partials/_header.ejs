<header>
  <h1><a href="/urls">TinyApp</a></h1>

  <% if (user) { %>
    <p>Logged in as: <%= user.email %></p>
    <form action="/logout" method="POST">
      <button type="submit">Logout</button>
    </form>
  <% } else { %>
    <a href="/login">Login</a>
    <a href="/register">Register</a>
  <% } %>
</header>
